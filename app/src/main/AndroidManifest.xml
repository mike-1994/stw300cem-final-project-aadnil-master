<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.digitalparking">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MyBooking"></activity>
        <activity android:name=".Dashboard2.New" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".Profile" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".About_Us" />
        <activity android:name=".Dashboard2.Home" />
        <activity android:name=".Maindashboard" />
        <activity android:name=".Available_Space" />
        <activity android:name=".Dashboard" />
        <activity android:name=".RegisterForm" />
        <activity android:name=".FeedbackActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginForm" />
    </application>

</manifest>